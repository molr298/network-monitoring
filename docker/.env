# Zabbix Configuration
ZABBIX_SERVER_IP=zabbix-server  # Use container name for Docker DNS resolution
ZABBIX_WEB_PORT=8080
ZABBIX_SERVER_PORT=10051
ZABBIX_AGENT_PORT=10050
ZABBIX_USER=Admin
ZABBIX_PASSWORD=zabbix

# Database
POSTGRES_USER=zabbix
POSTGRES_PASSWORD=zabbix
POSTGRES_DB=zabbix

# For external access, uncomment and set your host's IP:
# ZABBIX_SERVER_IP=your_host_ip

# Optional: Debug logging (set to 1 to enable)
ZBX_DEBUG=0


# Time zone (POSIX/TZDB name)
TZ=Asia/Ho_Chi_Minh

# Google AI API Key
GOOGLE_AI_API_KEY=